<!-- Box -->
<div class="box">
	<!-- Box Head -->
	<div class="box-head">
		<h2 class="left">Tenants</h2>
		<div class="right">
		</div>
  </div>
</div>

<div class="tlist">
<table width=100% border="0" cellpadding="0" cellspacing="0">
  <tr>
    <th>Location Name</th>
    <th>Staff Name</th>
    <th>Student Name</th>
    <th>Key Accept Date</th>
    <th>Key Expected Return Date</th>
    <th>Key Returned Date</th>
    <th>Force Vacate</th>
	<th colspan=4 class="ac">Control</th>
  </tr>

<% @tenants.each do |tenant| %>
  <tr>
    <td><%=h tenant.location_id %></td> <!-- 16/11/2011 - Shaliza added code for location if no longer exist.-->
    <td><%=h tenant.staff_name %></td> <!-- 16/11/2011 - Shaliza added code for staff if no longer exist.-->
    <td><%=h tenant.student_name %></td> <!-- 16/11/2011 - Shaliza added code for student if no longer exist.-->
    <td><%=h l(tenant.keyaccept) %></td>
    <td><%=h tenant.keyexpectedreturn %></td>
    <td><%=h tenant.keyreturned %></td>
    <td><%=h tenant.force_vacate %></td>
    <td width=10px ><%= link_to image_tag("document.png", :border => 0), :action => 'show', :id => tenant %></td>
    <td width=10px><%= link_to image_tag("edit.png", :border => 0), :action => 'edit', :id => tenant %></td>
    <td width=5px><%= link_to image_tag("delete.png",   :border => 0, :title => 'Delete'), tenant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
</div>
<br />

